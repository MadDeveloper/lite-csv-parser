{
  "name": "lite-csv-parser",
  "version": "1.0.0",
  "description": "A lite csv parser using promises",
  "main": "build.js",
  "scripts": {
    "start": "nodemon .",
    "start:prod": "npm run build && node .",
    "build": "babel parser.js --out-file build.js --presets=babel-preset-minify",
    "build:watch": "babel parser.js --watch --out-file build.js --presets=babel-preset-minify && npm run compress",
    "compress": "gzip < build.js > build.js.gz"
  },
  "keywords": [
    "csv",
    "parser",
    "promise",
    "lite",
    "node",
    "browser",
    "umd"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-preset-minify": "^0.5.1",
    "nodemon": "^1.19.3"
  }
}
